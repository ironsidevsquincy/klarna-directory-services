{
  "name": "Klaran Directory Services",
  "version": "1.0",
  "description": "Find out who owns a page",
  "manifest_version": 2,
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  }
}
